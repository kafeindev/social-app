{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "type-check": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {},
    "lint": {},
    "generate": {
      "dependsOn": ["^generate"],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "db:migrate:deploy": {},
    "db:push": {},
    "db:seed": {}
  }
}
