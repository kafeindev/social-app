datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id        String   @id @default(uuid())
    email     String   @unique
    username  String   @unique
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
